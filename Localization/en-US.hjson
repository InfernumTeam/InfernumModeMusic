		Items: {
			SoulDrivenHeadphones: {
				DisplayName: Soul-Driven Headphones
				Tooltip:
					'''
					Allows you to pick and play custom Infernum music of defeated bosses, and to toggle a special boss battle theme
					As you hold onto them, you vaguely feel a mystical presence
					You can also hear the sounds of instruments passionately playing
					'''
			}
		}

		HeadphonesText: {
			1: Now playing {0}!
			2: "{0} will be playing during boss fights!"
		}

